<suppressions>
  <!-- For our tests we do not demand API documentation. -->
  <suppress files="[/\\]src[/\\]test[/\\]java[/\\].*\.java" checks="Javadoc.*"/>

  <!-- Suppress checks for generated code. -->
  <suppress files="[/\\]target[/\\]" checks=".+"/>
  <suppress files=".*AutoValue_.*\.java$" checks=".+"/>
</suppressions>
